    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "minify-html-webpack-plugin": "^1.1.5",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
